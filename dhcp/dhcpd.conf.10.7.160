        pool {
            range 10.7.164.0 10.7.167.250;
            deny unknown-clients;
        }
    }

    # Equipamentos pessoais/nao cadastrados
    subnet 10.7.168.0 netmask 255.255.248.0 {
        option routers 10.7.168.1;
